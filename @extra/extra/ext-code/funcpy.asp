function PY_Str(xNum) 
'       -20319    -10247       0        160       255
' _________|_________|_________|_________|_________|
'   ---        vvv       ---       asc       ---    
Dim Str,i,a,b
  if xNum>0 and xNum<160 then 
    Str=chr(xNum) 
  else 
    if xNum<-20319 or xNum>-10247 then 
      Str=chr(xNum) 
    else 
      a=PY_Dic.Items 
      b=PY_Dic.Keys 
    for i=PY_Dic.count-1 to 0 step -1 
      if a(i)<=xNum then exit for 
    next 
      Str=b(i) 
    end if 
  end if 
PY_Str = Str
end function 

function Get_pyStr(xStr) 
Dim i,pyStr
  pyStr="" 
  for i=1 to len(xStr) 
    pyStr=pyStr&PY_Str(Asc(Mid(xStr,i,1))) 
  next 
Get_pyStr = pyStr
end function 

function Get_pyStr2(xStr) 
Dim i,pyStr,ch
  pyStr="" 
  for i=1 to len(xStr) 
    ch = PY_Str(Asc(Mid(xStr,i,1)))
    pyStr=pyStr& UCase(Left(ch,1))
  next 
Get_pyStr2 = pyStr
end function 

function Get_Asc(xStr) 
Dim i,pyStr
  pyStr="" 
  for i=1 to len(xStr) 
    pyStr=pyStr&Asc(Mid(xStr,i,1))&";"
  next 
Get_Asc = pyStr
end function 

