<script language="javascript" runat="server">
function Get_CodeAt(xStr) 
{
  var N, chn, ch, obj="";
  N = xStr.length;
  for(i=0;i<N;i++)
  {
    chn = xStr.charCodeAt(i);
	if(chn>127)
	{  
	   ch = "&#"+chn+";";
	   }else{
	   ch = xStr.substr(i,1);
	 }
	 obj = obj + ch;
  }
  return obj
}  
</script>

